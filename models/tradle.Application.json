  "required": ["applicant", "requestFor", "context"],
  "indexes": [
    {
      "hashKey": "context",
      "specificity": 5
    },
    {
      "hashKey": "applicant._permalink",
      "rangeKey": "_time",
      "specificity": 4
    },
    {
      "hashKey": "_t",
      "rangeKey": "_time",
      "specificity": 1
    },
    {
      "hashKey": "status",
      "rangeKey": ["requestFor", "_time"],
      "specificity": 3
    },
    {
      "hashKey": "assignedToTeam.id",
      "rangeKey": "_time",
      "specificity": 4
    },
    {
      "hashKey": "analyst._permalink",
