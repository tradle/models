{
  "id": "tradle.Passport",
  "type": "tradle.Model",
  "title": "Passport",
  "interfaces": [
    "tradle.Message"
  ],
  "subClassOf": "tradle.Form",
  "properties": {
    "_t": {
      "type": "string",
      "readOnly": true
    },
    "codeOfIssuing": {
      "type": "string"
    },
    "passportNumber": {
      "type": "number",
      "maxLength": 9,
      "minLength": 9,
      "displayName": true
    },
    "surname": {
      "type": "string"
    },
    "givenName": {
      "type": "string"
    },
    "nationality": {
      "type": "string"
    },
    "dateOfBirth": {
      "type": "date"
    },
    "sex": {
      "type": "string",
      "oneOf": [
        "Male",
        "Female"
      ]
    },
    "placeOfBirth": {
      "type": "string"
    },
    "dateOfIssue": {
      "type": "date"
    },
    "authority": {
      "type": "string",
      "displayName": true
    },
    "dateOfExpiry": {
      "type": "date",
      "displayName": true
    },
    "from": {
      "type": "object",
      "readOnly": true,
      "ref": "tradle.Identity",
      "displayName": true
    },
    "to": {
      "type": "object",
      "ref": "tradle.Identity",
      "readOnly": true
    },
    "time": {
      "type": "date",
      "readOnly": true
    },
    "photos": {
      "type": "array",
      "ref": "tradle.Photo",
      "inlined": true
    }
  },
  "required": [
    "photos",
    "codeOfIssuing",
    "passportNumber",
    "surname",
    "givenName",
    "nationality",
    "dateOfBirth",
    "sex",
    "placeOfBirth",
    "dateOfIssue",
    "authority",
    "dateOfExpiry"
  ],
  "viewCols": [
    "codeOfIssuing",
    "passportNumber",
    "surname",
    "givenName",
    "nationality",
    "dateOfBirth",
    "sex",
    "placeOfBirth",
    "dateOfIssue",
    "authority",
    "dateOfExpiry"
  ]
}