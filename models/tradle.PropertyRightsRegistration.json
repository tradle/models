{
  "id": "tradle.PropertyRightsRegistration",
  "title": "Property Rights Registration",
  "interfaces": [
    "tradle.Message",
    "tradle.Verifiable"
  ],
  "evidentiaryDocuments": [
    "tradle.Passport",
    "tradle.Visa",
    "tradle.DriverLicense",
    "tradle.IdentificationDetail"
  ],
  "subClassOf": "tradle.Form",
  "type": "tradle.Model",
  "properties": {
    "_t": {
      "type": "string",
      "readOnly": true
    },
    "property_group": {
      "type": "string",
      "title": "About property",
      "list": [
        "propertyType",
        "cadastralNumber",
        "conditionalNumber",
        "area"
      ]
    },
    "propertyType": {
      "type": "object",
      "ref": "tradle.TypesOfProperty"
    },
    "cadastralNumber": {
      "type": "string",
      "keyboard": "numbers-and-punctuation"
    },
    "conditionalNumber": {
      "type": "string",
      "keyboard": "numbers-and-punctuation"
    },
    "area": {
      "units": "mÂ²",
      "type": "number"
    },
    "address_group": {
      "type": "string",
      "title": "Property address",
      "list": [
        "fullAddress",
        "houseNumber",
        "unit",
        "building",
        "apartmentNumber",
        "other",
        "additionalInformation"
      ]
    },
    "fullAddress": {
      "type": "string"
    },
    "houseNumber": {
      "type": "string"
    },
    "unit": {
      "type": "string"
    },
    "building": {
      "type": "string"
    },
    "apartmentNumber": {
      "type": "string"
    },
    "other": {
      "type": "string"
    },
    "additionalInformation": {
      "type": "string"
    },
    "ownerType": {
      "type": "object",
      "ref": "tradle.OwnerType"
    },
    "owner_group": {
      "type": "string",
      "title": "About owner",
      "list": [
        "fullTitle",
        "PSRN",
        "TIN",
        "KPP",
        "ownerType",
        "registrationDate"
      ]
    },

    "fullTitle": {
      "type": "string"
    },
    "PSRN": {
      "type": "number",
      "title": "PSRN"
    },
    "TIN": {
      "type": "string",
      "title": "TIN",
      "description": "Tax identification number"
    },
    "KPP": {
      "type": "string",
      "title": "KPP",
      "description": "tax registration reason code"
    },
    "registrationDate": {
      "type": "date"
    },
    "ownerAddress_group": {
      "type": "string",
      "title": "Owner address",
      "list": [
        "ownerFullAddress",
        "ownerHouseNumber",
        "ownerHouseNumber",
        "ownerUnit",
        "ownerBuilding",
        "ownerApartmentNumber",
        "ownerOther"
      ]
    },
    "ownerFullAddress": {
      "type": "string"
    },
    "ownerHouseNumber": {
      "type": "string",
      "title": "House Number"
    },
    "ownerUnit": {
      "type": "string",
      "title": "Unit"
    },
    "ownerBuilding": {
      "type": "string",
      "title": "Building"
    },
    "ownerApartmentNumber": {
      "type": "string",
      "title": "Apartment number"
    },
    "ownerOther": {
      "type": "string",
      "title": "Other"
    },
    "contactInfo_group": {
      "type": "string",
      "title": "Contact info",
      "list": [
        "contactPhone",
        "contactEmailAddress"
      ]
    },
    "contactPhone": {
      "type": "array",
      "inlined": true,
      "items": {
        "ref": "tradle.Phone"
      }
    },
    "contactEmailAddress": {
      "type": "string",
      "pattern": "^(([^<>()\\[\\]\\\\.,;:\\s@\"]+(\\.[^<>()\\[\\]\\\\.,;:\\s@\"]+)*)|(\".+\"))@((\\[[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}\\.[0-9]{1,3}])|(([a-zA-Z\\-0-9]+\\.)+[a-zA-Z]{2,}))$",
      "keyboard": "email-address",
      "range": "email"
    }
  },
  "editCols": [
    "property_group",
    "address_group",
    "owner_group",
    "ownerAddress_group",
    "contactInfo_group"
  ],
  "required": [
    "propertyType",
    "cadastralNumber",
    "area",
    "fullAddress",
    "fullTitle",
    "TIN",
    "KPP",
    "PSRN",
    "registrationDate",
    "ownerFullAddress",
    "contactPhone"
  ],
  "viewCols": [
    "propertyType",
    "cadastralNumber",
    "conditionalNumber",
    "area",
    "fullAddress",
    "fullTitle",
    "TIN",
    "KPP",
    "PSRN",
    "registrationDate",
    "ownerFullAddress",
    "contactPhone"
  ]
}
