{
  "id": "tradle.DocumentPhotoID",
  "type": "tradle.Model",
  "title": "Document Photo ID",
  "subClassOf": "tradle.Form",
  "interfaces": [
    "tradle.Message"
  ],
  "properties": {
     "documentType": {
        "type": "object",
        "ref": "tradle.IDCardType"
     },
     "country": {
        "type": "object",
        "ref": "tradle.Country"
     },
     "scan": {
        "type": "object",
        "range": "photo",
        "component": "QRCodeScanner",
        "ref": "tradle.Photo"
     },
     "scanJson": {
        "type": "string",
        "range": "json",
        "readOnly": true
     },
    "scan_group": {
      "type": "string",
      "title": "Scanned Document",
      "list": [
        "scan",
        "scanJson"
      ]
    }
  },
  "required": [
    "documentType",
    "country",
    "scan",
    "scanJson"
  ],
  "viewCols": [
    "documentType",
    "country",
    "scan_group"
  ]
}