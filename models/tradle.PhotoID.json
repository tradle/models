{
  "id": "tradle.PhotoID",
  "type": "tradle.Model",
  "title": "Photo ID",
  "subClassOf": "tradle.Form",
  "interfaces": [
    "tradle.Verifiable",
    "tradle.Document"
  ],
  "verifiableAspects": {
    "authenticity": {
      "methods": [
        "tradle.VisualVerificationMethod"
      ]
    },
    "ownership": {
      "methods": [
        "tradle.VisualVerificationMethod"
      ]
    },
    "validity": {
      "methods": [
        "tradle.VisualVerificationMethod"
      ]
    }
  },
  "properties": {
    "_t": {
      "type": "string",
      "readOnly": true
    },
    "documentType": {
      "type": "object",
      "displayName": true,
      "ref": "tradle.IDCardType"
    },
    "country": {
      "type": "object",
      "title": "Country of Issue",
      "displayName": true,
      "ref": "tradle.Country"
    },
    "scan": {
      "type": "object",
      "range": "photo",
      "scanner": "id-document",
      "mainPhoto": true,
      "component": "BlinkID",
      "ref": "tradle.Photo",
      "sample": "scan.photoId"
    },
    "scanJson": {
      "type": "object",
      "title": "Extracted fields",
      "range": "json",
      "readOnly": true
    },
    "scannedBy": {
      "type": "object",
      "readOnly": true,
      "ref": "tradle.DocumentScanner"
    },
    "verifications": {
      "type": "array",
      "readOnly": true,
      "items": {
        "backlink": "document",
        "ref": "tradle.Verification"
      }
    },
    "personal_group": {
      "type": "string",
      "title": "Personal",
      "list": [
        "firstName",
        "lastName",
        "dateOfBirth"
      ]
    },
    "address_group": {
      "type": "string",
      "title": "Address",
      "list": [
        "full"
      ]
    },
    "document_group": {
      "type": "string",
      "title": "Document",
      "list": [
        "dateOfIssue",
        "documentNumber",
        "issuer",
        "dateOfExpiry"
      ]
    },
    "idCardDocument_group": {
      "type": "string",
      "title": "ID Card Details",
      "list": [
        "documentNumber",
        "issuer",
        "dateOfExpiry"
      ]
    },
    "firstName": {
      "type": "string"
    },
    "lastName": {
      "type": "string"
    },
    "full": {
      "type": "string"
    },
    "dateOfIssue": {
      "type": "date"
    },
    "documentNumber": {
      "type": "string"
    },
    "issuer": {
      "type": "string"
    },
    "dateOfExpiry": {
      "type": "date"
    },
    "sex": {
      "type": "object",
      "ref": "tradle.Sex"
    },
    "dateOfBirth": {
      "type": "date"
    },
    "nationality": {
      "type": "string"
    }
  },
  "required": [
    "documentType",
    "country",
    "scan"
  ],
  "hidden": [
    "scanJson"
  ],
  "editCols": [
    "documentType",
    "country",
    "scan"
  ],
  "viewCols": [
    "documentType",
    "country",
    "scan",
    "personal_group",
    "nationality",
    "sex",
    "document_group",
    "address_group"
  ]
}
