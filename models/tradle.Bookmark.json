{
  "id": "tradle.Bookmark",
  "type": "tradle.Model",
  "title": "Bookmark",
  "notEditable": true,
  "properties": {
    "message": {
      "type": "string",
      "title": "Title",
      "skipLabel": true,
      "displayName": true
    },
    "bookmark": {
      "type": "object",
      "range": "json",
      "readOnly": true
    },
    "grid": {
      "type": "boolean",
      "readOnly": true
    },
    "shared": {
      "type": "boolean",
      "description": "Check if you want to share this bookmark with others?"
    },
    "noInternalUse": {
      "type": "boolean",
      "readOnly": true
    },
    "folder": {
      "type": "object",
      "allowToAdd": true,
      "readOnly": true,
      "ref": "tradle.BookmarksFolder"
    }
  },
  "required": [
    "bookmark"
  ],
  "viewCols": [
    "bookmark"
  ]
}
