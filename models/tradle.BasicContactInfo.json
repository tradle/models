{
  "id": "tradle.BasicContactInfo",
  "title": "Basic Contact Info",
  "subClassOf": "tradle.Form",
  "type": "tradle.Model",
  "properties": {
    "firstName": {
      "type": "string",
      "sample": "name.firstName"
    },
    "lastName": {
      "type": "string",
      "sample": "name.lastName"
    },
    "email": {
      "type": "string",
      "range": "email",
      "keyboard": "email-address",
      "sample": "internet.email"
    },
    "formattedName": {
      "type": "string",
      "displayAs": "{1} {2}",
      "group": [
        "firstName",
        "lastName"
      ],
      "readOnly": true,
      "displayName": true
    }
  },
  "required": [
    "firstName",
    "lastName",
    "email"
  ],
  "viewCols": [
    "firstName",
    "lastName"
  ]
}
