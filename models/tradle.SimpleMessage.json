{
  "id": "tradle.SimpleMessage",
  "type": "tradle.Model",
  "title": "Simple Message",
  "autoCreate": true,
  "interfaces": [
    "tradle.ChatItem"
  ],
  "properties": {
    "message": {
      "type": "string",
      "displayName": true
    },
    "photos": {
      "type": "array",
      "range": "photo",
      "inlined": true,
      "items": {
        "ref": "tradle.Photo"
      }
    },
    "time": {
      "type": "date",
      "readOnly": true
    },
    "welcome": {
      "type": "boolean",
      "readOnly": true
    }
  },
  "required": [
    "message"
  ],
  "viewCols": [
    "message",
    "time"
  ]
}