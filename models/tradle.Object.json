{
  "type": "tradle.Model",
  "id": "tradle.Object",
  "title": "Object",
  "description": "the base Object model from which all inherit",
  "abstract": true,
  "properties": {
    "_t": {
      "type": "string",
      "readOnly": true,
      "description": "the instance type"
    },
    "_s": {
      "type": "string",
      "readOnly": true,
      "description": "signature"
    },
    "_r": {
      "type": "string",
      "readOnly": true,
      "description": "the link to the original version of this object (permalink)"
    },
    "_p": {
      "type": "string",
      "readOnly": true,
      "description": "the link to the previous version of this object"
    },
    "_q": {
      "type": "string",
      "readOnly": true,
      "description": "the link to the previous message sent to a given recipient"
    },
    "_n": {
      "type": "number",
      "readOnly": true,
      "description": "message sequence number, for tradle.Message object"
    },
    "_author": {
      "type": "string",
      "virtual": true,
      "readOnly": true,
      "description": "this object author's identity's permalink"
    },
    "_link": {
      "type": "string",
      "virtual": true,
      "readOnly": true
    },
    "_permalink": {
      "type": "string",
      "virtual": true,
      "readOnly": true
    },
    "_time": {
      "type": "number",
      "virtual": true,
      "readOnly": true
    },
    "_cut": {
      "type": "array",
      "virtual": true,
      "readOnly": true,
      "items": {
        "type": "string"
      }
    },
    "_displayName": {
      "type": "string",
      "virtual": true,
      "readOnly": true
    },
    "_sigPubKey": {
      "type": "string",
      "virtual": true,
      "readOnly": true
    },
    "_virtual": {
      "type": "array",
      "inlined": true,
      "items": {
        "type": "string"
      }
    }
  },
  "required": [
    "_t",
    "_s"
  ]
}
