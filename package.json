{
  "name": "@tradle/models",
  "version": "4.10.10",
  "description": "models for tradle apps",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --quiet --ext .js --ext .json .",
    "validate": "PRINT_ONLY=1 node ./validate -f ./models.json",
    "merge": "pack-models -i ./models -o ./models.json -d ./models -l bn,vi,en,nl,es,fr,fil",
    "dictionaries": "pack-models -d ./models -l ru",
    "postpublish": "git push -u",
    "precommit": "npm run merge && git add models.json && npm run validate && git add dictionary_en.json dictionary_bn.json dictionary_vi.json dictionary_es.json dictionary_fr.json dictionary_nl.json  dictionary_fil.json"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/tradle/models"
  },
  "keywords": [
    "tradle",
    "models"
  ],
  "author": "Mark Vayngrib <mark.vayngrib@lablz.com> (http://github.com/mvayngrib)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tradle/models/issues"
  },
  "homepage": "https://github.com/tradle/models",
  "devDependencies": {
    "@tradle/pack-models": "github:tradle/pack-models",
    "@tradle/validate-model": "^7.1.1",
    "eslint": "^3.15.0",
    "eslint-plugin-json": "^1.2.0",
    "husky": "^0.13.1",
    "json-stable-stringify": "^1.0.1",
    "minimist": "^1.2.0",
    "mockery": "^2.1.0",
    "world-countries": "git+https://github.com/mledoze/countries.git",
    "write-file-atomic": "^1.1.4"
  }
}
